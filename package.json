{
  "name": "MeGa_Downloader",
  "productName": "MeGa Downloader",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "node scripts/setEnv.js debug && electron-forge start",
    "prod": "node scripts/setEnv.js production && electron-forge start",
    "postInstallLinux": "sudo chmod 755 ./node_modules/youtube-dl/bin/youtube-dl",
    "package": "electron-forge package",
    "make": "node scripts/setEnv.js production && electron-forge make",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "konstantinos",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "icon": "public/download.ico",
        "version-string": {
          "ProductName": "MeGa Downloader",
          "CompanyName": "mgd",
          "FileDescription": "A simple mp3 converter"
        }
      },
      "electronWinstallerConfig": {
        "name": "MeGa-downloader"
      },
      "electronInstallerDebian": {
        "name": "MeGa-downloader"
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "mgdownloader"
      }
    }
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "babel-preset-react": "^6.22.0",
    "electron-compile": "^6.4.1",
    "fluent-ffmpeg": "2.1.2",
    "react": "^15.4.2",
    "react-desktop": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "youtube-dl": "^1.11.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "electron-prebuilt-compile": "1.6.11",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0"
  }
}
